{
    "manifest_version": 3,
    "name": "Satoru Gojo",
    "version": "1.0",
    "description": "I can expand the domain and change the world",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "toggle-design-mode": {
        "suggested_key": {
          "default": "Ctrl+E",
          "mac": "Command+E"
        },
        "description": "Toggle Design Mode"
      }
    }
  }
  